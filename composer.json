{
    "name": "sanjay-np/laravel-generator",
    "description": "Laravel package to generate actions, traits and services",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SanjayNp\\LaravelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SanjayNp\\LaravelGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanjayNp\\LaravelGenerator\\LaravelGeneratorServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "sanjay-np",
            "email": "excelblade10@gmail.com"
        }
    ]
}